@model IEnumerable<PhotoSharingApp.Model.Photo>

@foreach (var item in Model)
{
    <h3>"@item.Title"</h3>

    <img width="200"
         src="@Url.Action("GetImage",
 "Photo",
 new { id = item.PhotoID })" />

    <p>
        Created By:
        @Html.DisplayFor(modeltem => item.Owner )
    </p>
    <p>
        Created On: 
        @Html.DisplayFor(modelItem =>item.CreatedDate )
    </p>
    <p>
        @Html.ActionLink("Display" , "Display" ,new { id = item.PhotoID })
    </p>
    
}
